{
  "title": "log content",
  "id": "schemas/common/logContent",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "A parameter can specify if the content is plain or html. Either 'plain' or'html' is required",
  "oneOf": [
    {
      "required": [
        "plain"
      ]
    },
    {
      "required": [
        "html"
      ]
    }
  ],
  "properties": {
    "plain": {
      "type": "string"
    },
    "html": {
      "type": "string"
    }
  }
}
