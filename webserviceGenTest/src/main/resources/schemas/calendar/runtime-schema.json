{
  "title": "run time in calendar",
  "id": "schemas/calendar/runtime",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "",
  "required": [
    "path"
  ],
  "properties": {
    "path": {
      "description": "path of order or standalone job",
      "$ref": "../common/path-schema.json"
    },
    "orderId": {
      "description": "order id",
      "type": "string"
    },
    "state": {
      "type": "string",
      "enum": [
        "pending",
        "running",
        "suspended",
        "setback",
        "waitingForResource",
        "blacklist"
      ],
      "javaEnumNames": [
        "pending",
        "running",
        "suspended",
        "setback",
        "waitingForResource",
        "blacklist"
      ]
    },
    "holiday": {
      "type": "boolean",
      "default": false
    },
    "startTime": {
      "$ref": "../common/estimatedTime-schema.json"
    },
    "endTime": {
      "$ref": "../common/estimatedTime-schema.json"
    }
  }
}
