{
  "title": "jobscheduler state",
  "id": "schemas/jobscheduler/state",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "severity",
    "_text"
  ],
  "properties": {
    "severity": {
      "description": "0=running, 1=paused, 3=waiting_for_activation/terminating, 2=waiting_for_database/dead/unreachable",
      "type": "integer"
    },
    "_text": {
      "type": "string",
      "enum": [
        "running",
        "paused",
        "waiting_for_activation",
        "terminating",
        "waiting_for_database",
        "dead",
        "unreachable"
      ],
      "javaEnumNames": [
        "running",
        "paused",
        "waiting_for_activation",
        "terminating",
        "waiting_for_database",
        "dead",
        "unreachable"
      ]
    }
  }
}
