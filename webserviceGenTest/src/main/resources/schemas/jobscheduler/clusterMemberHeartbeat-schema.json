{
  "title": "cluster member heartbeat",
  "id": "schemas/jobscheduler/clusterMemberHeartbeat",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "count",
    "quality"
  ],
  "properties": {
    "count": {
      "type": "integer"
    },
    "quality": {
      "type": "string",
      "enum": [
        "good",
        "late",
        "still_checking"
      ],
      "javaEnumNames": [
        "good",
        "late",
        "still_checking"
      ]
    },
    "lastDetected": {
      "type": "string",
      "format": "date-time"
    },
    "lastDetectedAge": {
      "type": "integer"
    },
    "dead": {
      "type": "boolean",
      "default": false
    }
  }
}
