{
  "title": "jobChainConfigurationFilter",
  "id": "schemas/jobChain/jobChainConfigurationFilter",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "jobChain"
  ],
  "properties": {
    "jobschedulerId": {
      "type": "string"
    },
    "jobChain": {
      "$ref": "../common/path-schema.json"
    },
    "mime": {
      "description": "The configuration can have a HTML representation where the XML gets a highlighting via CSS classes.",
      "type": "string",
      "enum": [
        "xml",
        "html"
      ],
      "javaEnumNames": [
        "xml",
        "html"
      ],
      "default": "xml"
    }
  }
}
