{
  "title": "fileSink",
  "id": "schemas/jobchain/fileSinkNode",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "job chain node object of a file sink, 'remove' or 'move' are exclusivly",
  "required": [
    "name"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "remove": {
      "type": "boolean",
      "default": false
    },
    "move": {
      "description": "a directory path is expected",
      "type": "string",
      "minLength": 1
    }
  }
}
