{
  "title": "categories",
  "id": "schemas/calendar/categories",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.calendar.Dates",
  "type": "object",
  "required": [
    "deliveryDate",
    "path",
    "name",
    "dates"
  ],
  "properties": {
    "deliveryDate": {
      "$ref": "../common/deliveryDate-schema.json"
    },
    "path": {
      "$ref": "../common/path-schema.json"
    },
    "name": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "days": {
      "type": "array",
      "items": {
        "$ref": "../common/date-schema.json"
      }
    },
    "holidays": {
      "type": "array",
      "items": {
        "$ref": "../common/date-schema.json"
      }
    }
  }
}
