{
  "title": "frequencies",
  "id": "schemas/calendar/frequencies",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.calendar.Frequencies",
  "type": "object",
  "required": [
    "mode"
  ],
  "properties": {
    "mode": {
      "type": "string",
      "javaType": "com.sos.joc.model.calendar.FrequencyMode",
      "enum": [
        "INCLUDE",
        "EXCLUDE"
      ]
    },
    "dates": {
      "type": "array",
      "items": {
        "$ref": "../common/date-schema.json"
      }
    },
    "weekdays": {
      "$ref": "weekdays-schema.json"
    },
    "ultimos": {
      "type": "array",
      "items": {
        "$ref": "ultimo-schema.json"
      }
    },
    "monthdays": {
      "$ref": "monthdays-schema.json"
    },
    "months": {
      "type": "array",
      "items": {
        "$ref": "month-schema.json"
      }
    },
    "holidays": {
      "type": "array",
      "items": {
        "$ref": "holiday-schema.json"
      }
    },
    "intervals": {
      "type": "array",
      "items": {
        "$ref": "every-schema.json"
      }
    }
  }
}
