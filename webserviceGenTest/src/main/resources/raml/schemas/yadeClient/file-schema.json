{
  "title": "yade file",
  "id": "schemas/yadeClient/file",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.DataExchange.model.TransferFile",
  "type": "object",
  "required": [
    "transferId",
    "sourcePath"
  ],
  "properties": {
    "transferId": {
      "type": "string",
      "format":"utc-millisec"
    },
    "stateText": {
      "javaType": "com.sos.DataExchange.model.FileTransferStateText",
      "type": "string",
      "enum": [
        "UNDEFINED",
        "WAITING",
        "TRANSFERRING",
        "IN_PROGRESS",
        "TRANSFERRED",
        "SUCCESS",
        "SKIPPED",
        "FAILED",
        "ABORTED",
        "COMPRESSED",
        "NOT_OVERWRITTEN",
        "DELETED",
        "RENAMED",
        "IGNORED_DUE_TO_ZEROBYTE_CONSTRAINT",
        "SETBACK",
        "POLLING"
      ]
    },
    "integrityHash": {
      "type": "string"
    },
    "modificationDate": {
      "type": "string",
      "format": "date-time"
    },
    "size": {
      "type": "string",
      "format":"utc-millisec"
    },
    "error": {
      "$ref": "error-schema.json"
    },
    "sourcePath": {
      "type": "string"
    },
    "targetPath": {
      "type": "string"
    }
  }
}
