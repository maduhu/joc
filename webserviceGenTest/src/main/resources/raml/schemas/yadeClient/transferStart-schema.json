{
  "title": "transfer start",
  "id": "schemas/yadeClient/transferStart",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.DataExchange.model.TransferStart",
  "type": "object",
  "required": [
    "_operation",
    "start",
    "source"
  ],
  "properties": {
    "profile": {
      "type": "string"
    },
    "mandator": {
      "type": "string"
    },
    "_operation": {
      "type": "string",
      "javaType": "com.sos.DataExchange.model.Operation",
      "enum": [
        "COPY",
        "MOVE",
        "GETLIST",
        "RENAME"
      ]
    },
    "start": {
      "type": "string",
      "format": "date-time"
    },
    "end": {
      "type": "string",
      "format": "date-time"
    },
    "configuration": {
      "type": "string"
    },
    "log": {
      "type": "string"
    },
    "error": {
      "$ref": "error-schema.json"
    },
    "source": {
      "$ref": "protocolFragment-schema.json"
    },
    "target": {
      "$ref": "protocolFragment-schema.json"
    },
    "jump": {
      "$ref": "protocolFragment-schema.json"
    },
    "numOfFiles": {
      "type": "integer",
      "minimum": 0
    },
    "jobschedulerId": {
      "type": "string"
    },
    "orderId": {
      "type": "string"
    },
    "jobChain": {
      "type": "string"
    },
    "job": {
      "type": "string"
    }
  }
}
