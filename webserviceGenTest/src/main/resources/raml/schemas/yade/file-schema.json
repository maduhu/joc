{
  "title": "yade file",
  "id": "schemas/yade/file",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.yade.TransferFile",
  "type": "object",
  "description": "compact=true -> required fields + possibly targetPath",
  "required": [
    "surveyDate",
    "id",
    "transferId",
    "interventionTransferId",
    "state",
    "size",
    "sourcePath"
  ],
  "properties": {
    "surveyDate": {
      "$ref": "../common/surveyDate_p-schema.json"
    },
    "id": {
      "$ref": "../common/nonNegativeLong-schema.json"
    },
    "transferId": {
      "$ref": "../common/nonNegativeLong-schema.json"
    },
    "interventionTransferId": {
      "$ref": "../common/nonNegativeLong-schema.json"
    },
    "state": {
      "$ref": "fileTransferState-schema.json"
    },
    "integrityHash": {
      "type": "string"
    },
    "modificationDate": {
      "type": "string",
      "format": "date-time"
    },
    "size": {
      "$ref": "../common/nonNegativeLong-schema.json"
    },
    "error": {
      "$ref": "../common/error-schema.json"
    },
    "sourcePath": {
      "type": "string"
    },
    "targetPath": {
      "type": "string"
    }
  }
}
