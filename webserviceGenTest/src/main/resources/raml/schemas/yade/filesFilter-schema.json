    "transferIds": {
      "type": "array",
      "items": {
        "$ref": "../common/nonNegativeInteger-schema.json"
      }
    },
    "interventionTransferIds": {
      "type": "array",
      "items": {
        "$ref": "../common/nonNegativeInteger-schema.json"
      }
    },
    "compact": {
